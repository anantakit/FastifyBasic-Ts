{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "arrow-return-shorthand": true,
    "quotemark": [true, "single"],
    "no-debugger": true,
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "no-async-without-await": true
  },
  "rulesDirectory": []
}
